{
  "routes": [
    {
      "src": "/about",
      "dest": "/index.html"
    },
    {
      "src": "/portfolio",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
